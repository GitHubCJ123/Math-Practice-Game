{
  "name": "math-practice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch api/server.ts",
    "start": "concurrently \"npm:dev\" \"npm:dev:api\"",
    "dev:test": "cross-env VITE_NODE_ENV=test vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "node server.cjs",
    "server:test": "cross-env NODE_ENV=test node -r dotenv/config server.cjs dotenv_config_path=.env.test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.12",
    "@google/genai": "^0.15.0",
    "@types/react-router-dom": "^5.3.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "node-cron": "^4.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "tedious": "^18.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^22.14.0",
    "@types/tedious": "^4.0.14",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "cypress": "^15.5.0",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vercel": "^48.6.0",
    "vite": "^6.2.0"
  }
}
